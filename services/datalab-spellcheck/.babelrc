{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "6.10.0"
        },
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "babel-plugin-inline-import",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties"
  ]
}
